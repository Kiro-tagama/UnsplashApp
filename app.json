{
  "expo": {
    "name": "unsplashApp",
    "slug": "unsplashapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "unsplashapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow unsplashApp to access your photos.",
          "savePhotosPermission": "Allow unsplashApp to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4afd3d76-d3ba-47e5-9430-6ea562eb2b25"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kirotagama.expotemplatetabs"
    },
    "android": {
      "adaptiveIcon": {},
      "package": "com.kirotagama.expotemplatetabs",
      "permissions":["READ_EXTERNAL_STORAGE","WRITE_EXTERNAL_STORAGE","INTERNET","MEDIA_LIBRARY"]
    },
    "extra": {
      "eas": {
        "projectId": "4afd3d76-d3ba-47e5-9430-6ea562eb2b25"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
